{
  "name": "gabriel",
  "description": "g",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "npm run build:css && qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build --ssr src/entry.cloudflare-pages.tsx",
    "build.types": "tsc --incremental --noEmit",
    "build:css": "tailwindcss -m -i ./src/styles/global.css -o ./src/styles/build.css",
    "dev": "concurrently \"bun run dev:css\" \"vite --mode ssr\"",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev:css": "tailwindcss -w -i ./src/styles/global.css -o ./src/styles/build.css",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.10.0",
    "@builder.io/qwik-city": "^0.0.112",
    "@catppuccin/tailwindcss": "^0.1.0",
    "@types/eslint": "8.4.6",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "^10.4.12",
    "bun": "^0.0.12",
    "concurrently": "^7.4.0",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "^0.10.0",
    "node-fetch": "3.2.10",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "beta"
  },
  "dependencies": {
    "@fontsource/corben": "^4.5.9",
    "@fontsource/poppins": "^4.5.9"
  }
}